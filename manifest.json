{
    "name": "Seterra extra",
    "version": "1.0",
    "description": "Some cool features for seterra",
    "host_permissions": ["https://*.geoguessr.com/vgp/*"],
    "permissions": ["activeTab", "storage"],
    "manifest_version": 3,
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.geoguessr.com/vgp/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }